                <State name="shift">
                    <Action/>                               <Action text="↑" code="%1b[1;2A"/>      <Action/>
                    <Action text="←" code="%1b[1;2D"/>      <Action text="↲" key="66"/>             <Action text="→" code="%1b[1;2C"/>

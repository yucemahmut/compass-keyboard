                <State name="spec">
                    <Action/>                               <Action text="↟" key="92"/>             <Action/>
                    <Action text="↞" code="%1b[H"/>         <Action text="╳" key="23"/>             <Action text="↠" code="%1b[F"/>
                    <Action/>                               <Action text="↡" key="93"/>             <Action/>
